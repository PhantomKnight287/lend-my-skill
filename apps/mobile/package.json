{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
  },
  "dependencies": {
    "@expo-google-fonts/overpass": "^0.2.3",
    "@react-native-async-storage/async-storage": "^1.17.12",
    "expo": "~48.0.6",
    "expo-app-loading": "^2.1.1",
    "expo-font": "~11.1.1",
    "expo-router": "^1.3.0",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.3",
    "react-native-web": "~0.18.10",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.0.14",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.4"
  },
  "private": true,
  "overrides": {
    "metro": "0.73.7",
    "metro-resolver": "0.73.7"
  }
}
